package prob;
import java.util.Scanner;
public class Main {
public static void main(String[] args) {
	Scanner sc = new Scanner(System.in);
	int m=0;
	int n=0;
		for(int c=1; c<10; c++){
			int B = sc.nextInt();
			if(m<B){
				m=B;
				n=c;
			}
		}
		System.out.println(m);
		System.out.println(n);
    }
}
