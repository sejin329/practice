package prob;
import java.util.Scanner;
public class Main {
public static void main(String[] args) {
	Scanner sc = new Scanner(System.in);
	int A = sc.nextInt();
	int B = A;
	int C = 0;
		while(true) {
			int left = B/10;
			int right = B%10;
			B =(left + right)%10+right*10;
			C++;
			if(A==B) {
				break;
        	}
        }
        System.out.println(C);
    }
}
